{
    "version": 2,
    "builds": [
      {
        "src": "next.config.js", 
        "use": "@vercel/node" 
      },
            {
        "src": "public/**/*", 
        "use": "@vercel/static" 
      }
    ],
    "routes": [
      {
        "src": "/api/v1/(.*)", 
        "dest": "https://tykkera.onrender.com/api/v1/$1" 
      },
      {
        "src": "/(.*)", 
        "dest": "/index.html" 
      }
    ]
  }
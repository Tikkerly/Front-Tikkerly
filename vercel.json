{
    "version": 2,
    "builds": [
      {
        "src": "next.config.js", 
        "use": "@vercel/node"
      },
      {
        "src": ".next/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/v1/(.*)", 
        "dest": "https://back-tikkerly.vercel.app/api/v1" 
      }
    ]
  }